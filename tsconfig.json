{
    "compilerOptions": {
        "lib": [
            "es2023"
        ],
        "module": "AMD",
        "target": "es2022",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "moduleResolution": "Node"
    },
    "include": [
        "app/**/*.ts"
, "app/src/main.ts"    ],
    "exclude": [
        "node_modules",
        "app/test/**/*.ts"
    ]

}